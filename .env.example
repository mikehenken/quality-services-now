# Google API Keys
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Cloudflare API Credentials
# Get from: https://dash.cloudflare.com/profile/api-tokens
CLOUDFLARE_API_KEY=your_cloudflare_api_key_here
CLOUDFLARE_GLOBAL_TOKEN=your_cloudflare_global_token_here
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_TOKEN=your_cloudflare_account_token_here
CLOUDFLARE_ORIGINAL_TOKEN=your_cloudflare_original_token_here

# Cloudflare Account Info
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_EMAIL=your_email@example.com

# Web3Forms (already set in Cloudflare Pages env vars)
NEXT_PUBLIC_WEB3FORMS_KEY=your_web3forms_key_here
